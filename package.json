{
	"name": "audic",
	"version": "0.0.0",
	"description": "Play some audio.",
	"repository": "https://github.com/Richienb/audic.git",
	"author": "Richie Bendall <richiebendall@gmail.com>",
	"license": "MIT",
	"main": "dist/index.js",
	"browser": "dist/browser.js",
	"files": [
		"source/**/*",
		"dist/**/*"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"docs": "typedoc",
		"build": "tsc && yarn docs",
		"dev": "yarn tsc --watch",
		"lint": "xo",
		"test": "yarn lint && ava"
	},
	"keywords": [
		"audio",
		"vlc",
		"play",
		"speaker",
		"encode",
		"process",
		"pause",
		"resume",
		"playlist",
		"api",
		"interface",
		"audio",
		"html",
		"element"
	],
	"dependencies": {
		"@richienb/vlc": "^1.0.2",
		"ow": "^0.17.0",
		"type-fest": "^0.12.0"
	},
	"devDependencies": {
		"@sindresorhus/is": "^2.1.0",
		"ava": "^3.3.0",
		"eslint-config-richienb": "^0.3.0",
		"ts-node": "^8.6.2",
		"typedoc": "^0.16.10",
		"typescript": "^3.8.2",
		"xo": "^0.26.1"
	},
	"resolutions": {
		"eslint": "^6.8.0"
	},
	"xo": {
		"extends": "richienb/ts"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}
}
